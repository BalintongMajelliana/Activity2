class Book:
    def __init__(self, title, author, year_published):
        self.title = title
        self.author = author
        self.year_published = year_published

    def describe(self):
        print(f"Book: {self.title} by {self.author} ({self.year_published})")

# Create three book objects
book1 = Book("The Alchemist", "Paulo coelho", 1988)
book2 = Book("The catcher in the Rye", "J.D. Salinger",  1951)
book3 = Book("The Great Gatsby", "F. Scott Fitzgerald", 1925)

# Display book details
book1.describe()
book2.describe()
book3.describe()